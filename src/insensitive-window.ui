  <object class="GtkTextBuffer" id="pulseProgram_textbuffer">
    <property name="text" translatable="yes">;cosygpqf
;insensitive-version 0.9.33
;
;DISCLAIMER: This pulse program was created automatically for
;educational purposes only. It may be incomplete or contain
;errors. Do not run it on real spectrometer hardware without
;checking the code with an experienced and authorised person.
;The author takes absolutely no warranty for damaged equipment!
;
;$CLASS=
;$DIM=2D
;$TYPE=
;$SUBTYPE=
;$COMMENT=


#include &lt;Avance.incl&gt;
#include &lt;Grad.incl&gt;


"d13=4u"


"in0=inf1"

"d0=3u"


1 ze
2 d1
3 p1 ph1
  d0
  50u UNBLKGRAD
  p16:gp1
  d16
  p1 ph2
  d13
  p16:gp1
  d16
  4u BLKGRAD
  go=2 ph31
  d1 mc #0 to 2 F1QF(caldel(d0, +in0))
exit


ph1=0 2 0 2 0 0 0 0
ph2=2 0 2 2 2 2 2 2
ph31=0 2 0 2 0 0 0 0


;pl1 : f1 channel - power level for pulse (default)
;p1 : f1 channel - 90 degree high power pulse
;p16: homospoil/gradient pulse
;d0 : incremented delay (2D)                         [3 usec]
;d1 : relaxation delay 1-5 * T1
;d13: short delay                                    [4 usec]
;d16: delay for homospoil/gradient recovery
;nd0 = 1
;inf1: 1/SW = 2 * DW
;in0: 1/(1 * SW) = 2 * DW
;ns: 1 * n, total number of scans: NS * TD0
;ds: 16
;td1: number of experiments
;FnMODE: QF


;use gradient ratio:    gp 1
;                        100

;for z-only gradients:
;gpz1: 100%

;use gradient files:
;gpnam1: SMSQ10.100



;$Id: &lt;no name&gt;,v 1.0 2021/10/31 7:26:07 ber Exp $</property>
  </object>
